{
    "name": "zikula/trivial-module",
    "version": "1.0.0",
    "description": "ZikulaTrivialModule generated by ModuleStudio 1.2.0.",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Krator",
            "email": "kratord@hotmail.com",
            "homepage": "https://www.heroesofmightandmagic.es",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\TrivialModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "*"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=2.0.1 <3.0",
            "class": "Zikula\\TrivialModule\\ZikulaTrivialModule",
            "displayname": "Trivial",
            "url": "trivial",
            "capabilities": {
                "user": {"route": "zikulatrivialmodule_tournament_index"},
                "admin": {"route": "zikulatrivialmodule_tournament_adminindex"},
                "categorizable": {
                    "entities": [
                        "Zikula\\TrivialModule\\Entity\\QuestionEntity"
                    ]
                }
            },
            "securityschema": {
                "ZikulaTrivialModule::": "::",
                "ZikulaTrivialModule:ItemListBlock:": "Block title::",
                "ZikulaTrivialModule:ItemBlock:": "Block title::",
                "ZikulaTrivialModule:Tournament:": "Tournament ID::",
                "ZikulaTrivialModule:Question:": "Question ID::",
                "ZikulaTrivialModule:Tournament:Question": "Tournament ID:Question ID:",
                "ZikulaTrivialModule:Answer:": "Answer ID::",
                "ZikulaTrivialModule:Question:Answer": "Question ID:Answer ID:",
                "ZikulaTrivialModule:Result:": "Result ID::",
                "ZikulaTrivialModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "platform": {
            "php": "5.5.9"
        }
    }
}
