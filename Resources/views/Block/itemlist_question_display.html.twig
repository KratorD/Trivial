{# Purpose of this template: Display questions within an external context #}
{% for question in items %}
    <h3>{{ question|zikulatrivialmodule_formattedTitle }}</h3>
    <p><a href="{{ path('zikulatrivialmodule_question_display', {'id': question.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
